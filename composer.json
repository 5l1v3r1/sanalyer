{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "artesaos/seotools": "^0.11.1",
        "barryvdh/laravel-cors": "^0.11.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.2.1",
        "fideloper/proxy": "~4.0",
        "folklore/image": "0.3.*",
        "genealabs/laravel-model-caching": "^0.3.3",
        "imanghafoori/laravel-widgetize": "^1.8",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelium/feed": "^2.11",
        "laravelium/sitemap": "^2.7",
        "lullabot/amp": "^1.0.0",
        "predis/predis": "^1.1",
        "toolkito/larasap": "^1.2",
        "tymon/jwt-auth": "^0.5.12",
        "urb/xenforobridge": "dev-master",
        "uxweb/sweet-alert": "^1.4",
        "yajra/laravel-datatables-oracYANLIÅžle": "~8.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Radkod\\Posts\\": "packages/radkod/posts/src",
            "Radkod\\Xenforo2\\": "packages/radkod/xenforo2/src",
            "Radkod\\Admin\\": "packages/radkod/admin/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "Intervention/image"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
